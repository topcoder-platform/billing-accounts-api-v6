#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/jmgasper/Documents/Git/billing-accounts-api-v6/node_modules/.pnpm/bunyan@1.8.15/node_modules/bunyan/bin/node_modules:/home/jmgasper/Documents/Git/billing-accounts-api-v6/node_modules/.pnpm/bunyan@1.8.15/node_modules/bunyan/node_modules:/home/jmgasper/Documents/Git/billing-accounts-api-v6/node_modules/.pnpm/bunyan@1.8.15/node_modules:/home/jmgasper/Documents/Git/billing-accounts-api-v6/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/jmgasper/Documents/Git/billing-accounts-api-v6/node_modules/.pnpm/bunyan@1.8.15/node_modules/bunyan/bin/node_modules:/home/jmgasper/Documents/Git/billing-accounts-api-v6/node_modules/.pnpm/bunyan@1.8.15/node_modules/bunyan/node_modules:/home/jmgasper/Documents/Git/billing-accounts-api-v6/node_modules/.pnpm/bunyan@1.8.15/node_modules:/home/jmgasper/Documents/Git/billing-accounts-api-v6/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../bunyan/bin/bunyan" "$@"
else
  exec node  "$basedir/../bunyan/bin/bunyan" "$@"
fi
